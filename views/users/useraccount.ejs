<script>
    function deleteAddress(addressId) {
        console.log(addressId);
        Swal.fire({
            title: "Are you sure?",
            text: `You want to Delete Address!`,
            icon: "warning",
            showCancelButton: true,
            confirmButtonColor: "#3085d6",
            cancelButtonColor: "#d33",
            confirmButtonText: `Yes, delete it!`
        }).then((result) => {
            if (result.isConfirmed) {
                fetch(`/deleteaddress/${addressId}`, { method: "GET" })
                    .then(response => {
                        if (response.ok) {
                            Swal.fire({
                                title: `delete`,
                                text: `address were deleted`,
                                icon: "success"
                            });
                            setTimeout(() => {
                                location.reload()
                            }, 2000);
                        } else {
                            Swal.fire({
                                title: "Error!",
                                text: "Somthing Went Wrong.",
                                icon: "error"
                            });
                        }
                    })

            }
        });
    }
</script>


<script>
    function cancelOrder(orderId) {
    console.log("Cancel order clicked for orderId:", orderId);

    // Let's check the type of orderId
    console.log("Type of orderId:", typeof orderId);

    Swal.fire({
        title: "Are you sure?",
